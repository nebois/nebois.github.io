{
    "output": "./docs",
    "plugins": [
        "-lunr",
        "-search", 
        "search-pro",  
        "chapter-fold",
        "expandable-chapters",
        "splitter",
        "edit-link",
        "hide-element",
        "tbfed-pagefooter"
    ],

    "pluginsConfig": {
      "edit-link": {
            "base": "//github.com/nebois/nebois.github.io/edit/main",
            "label": "编辑此页面"
       },
       "hide-element": {
            "elements": [".gitbook-link"]
        },
        "tbfed-pagefooter": {
            "copyright":"",
            "modify_label": "该文件最后修改时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}